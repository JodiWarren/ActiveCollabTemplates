{
  "manifest_version": 2,

  "name": "Active Collab Templates",
  "description": "This extension adds an AC comment with a custom template",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Set comment"
  },
  "permissions": [
    "activeTab",
    "https://app.activecollab.com/*"
  ]
}